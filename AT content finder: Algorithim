#ACTGATCGATTACGTATAGTATTTGCTATCATACATATATATCGATGCGTTCAT

#create the string that will be used
atcontent = "ACTGATCGATTACGTATAGTATTTGCTATCATACATATATATCGATGCGTTCAT"

countera = atcontent.count('A')
countert = atcontent.count('T')
counter = (countera + countert)
print("Count of AT in ACTGATCGATTACGTATAGTATTTGCTATCATACATATATATCGATGCGTTCAT is : " , counter)
